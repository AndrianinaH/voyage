<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="S6.voyage.baseModel.Client" table="client">
        <meta attribute="class-description">
            This class contains the client detail.
        </meta>
        <id name="id" type="int" column="id_client">
            <generator class="identity">
            	<param name="sequence">client_id_client_seq</param>
            </generator>
        </id>
        <property name="nom_client" column="nom_client" type="string"/>
        <property name="email" column="email" type="string"/>
        <property name="mdp" column="mdp" type="string"/>
    </class>
</hibernate-mapping>