<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="S6.voyage.baseModel.User" table="users">
        <meta attribute="class-description">
            This class contains the user detail.
        </meta>
        <id name="id" type="int" column="id_user">
            <generator class="identity">
            	<param name="sequence">users_id_user_seq</param>
            </generator>
        </id>
        <property name="nom_user" column="nom_user" type="string"/>
        <property name="mdp_user" column="mdp_user" type="string"/>
    </class>
</hibernate-mapping>